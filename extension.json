{
  "name": "FancyBoxThumbs",
  "author": "Jason Gill, Dolfinus",
  "url": "http://www.mediawiki.org/wiki/Extension:FancyBoxThumbs",
  "description": "Displays thumbnailed images in a Mac-style lightbox that floats overtop of web page. A simple and fancy lightbox alternative",
  "version": "2.1",
  "license-name": "MIT",
  "type": "media",
  "ResourceModules": {
    "ext.FancyBoxThumbs": {
      "position": "top",
      "targets": [
        "desktop"
      ],
      "scripts": [
        "jquery.mousewheel.js",
        "jquery.fancybox.js",
        "fancyBoxThumbs.js"
      ],
      "styles": [
        "jquery.fancybox.css"
      ]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "modules",
    "remoteExtPath": "FancyBoxThumbs/modules"
  },
  "AutoloadClasses": {
    "FancyBoxThumb": "FancyBoxThumbs_body.php"
  },
  "Hooks": {
    "BeforePageDisplay": [
      "FancyBoxThumb::fbtBeforePageDisplay"
    ]
  },
  "manifest_version": 1
}